<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="false" />
        <rewrite>
            <rules>
                <rule name="Plesk. Wordpress redirect wpConfigRule #51024217-1fac-487e-ba0c-5ecda9ab21cf" stopProcessing="true">
                    <match url="^wp-config.php$" ignoreCase="false" />
